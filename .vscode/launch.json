  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to Node Version >= 7.7",
      "type": "node",
      "request": "attach",
      "localRoot": "${workspaceRoot}",
      "remoteRoot": "/app",
    },
    {
      "name": "Docker: Attach to Node Version <= 7.6",
      "type": "node",
      "request": "attach",
      "localRoot": "${workspaceRoot}",
      "remoteRoot": "/app",
      "protocol": "legacy",
      "port": 5858
    }
  ]
}
